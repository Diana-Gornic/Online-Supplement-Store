<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="proiect.fis.store.controllers.DeliveryStatusController">
    <children>
        <TableView fx:id="myTable" layoutX="31.0" layoutY="30.0" prefHeight="350.0" prefWidth="400.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="120.0" text="Name" />
                <TableColumn fx:id="priceColumn" prefWidth="70.0" text="Price" />
                <TableColumn fx:id="quantityColumn" prefWidth="80.0" text="Quantity" />
                <TableColumn fx:id="statusColumn" prefWidth="135.0" text="Status" />
            </columns>
        </TableView>
        <Button layoutX="495.0" layoutY="340.0" mnemonicParsing="false"
                fx:id="backToMainButton" onAction="#backToMain"
                text="Back to Main" />


    </children>
</AnchorPane>