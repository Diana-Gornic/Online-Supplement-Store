<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" minHeight="650.0" minWidth="750.0" prefHeight="600.0" prefWidth="700.0" style="-fx-background-color: #37084A;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="proiect.fis.store.controllers.LoginController">
   <children>
      <TextField fx:id="usernameInput" layoutX="191.0" layoutY="228.0" prefWidth="270.0" promptText="Username" style="-fx-background-color: transparent; -fx-border-color: #B670D2; -fx-background-insets: 0; -fx-border-radius: 20; -fx-text-inner-color: #B670D2;" />
      <PasswordField fx:id="passwordInput" layoutX="191.0" layoutY="272.0" onKeyReleased="#setButtonProperties" prefWidth="270.0" promptText="Password" style="-fx-background-color: transparent; -fx-border-color: #B670D2; -fx-background-insets: 0; -fx-border-radius: 20; -fx-text-inner-color: #B670D2;" />
      <Button fx:id="toRegisterBtn" layoutX="190.0" layoutY="362.0" mnemonicParsing="false" onAction="#switchToRegister" prefWidth="270.0" style="-fx-background-color: transparent; -fx-border-color: #B670D2; -fx-border-radius: 20; -fx-text-fill: #B670D2;" text="Register here" />
      <CheckBox fx:id="checkButton" layoutX="261.0" layoutY="314.0" mnemonicParsing="false" style="-fx-text-fill: #B670D2; -fx-font-size: 1.5em; -fx-background-color: transparent;" text="I'm a supplier" />
      <Button fx:id="loginButton" layoutX="191.0" layoutY="412.0" mnemonicParsing="false" onAction="#logIn" prefWidth="270.0" style="-fx-background-color: transparent; -fx-border-color: #B670D2; -fx-text-fill: #B670D2; -fx-border-radius: 20;" text="Login" />
      <Label layoutX="236.0" layoutY="53.0" style="-fx-text-fill: #B670D2; -fx-font-size: 3em;" text="WELCOME!" />
      <Label layoutX="119.0" layoutY="107.0" style="-fx-text-fill: #B670D2; -fx-font-size: 3em;" text="PLEASE LOGIN OR REGISTER!" />
   </children>

</AnchorPane>
